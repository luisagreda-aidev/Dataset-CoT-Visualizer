{
    "name": "dataset-cot-visualizer",
    "version": "1.1.5",
    "description": "Visualizador de Datasets Generados por Luis Agreda",
    "main": "main.js",
    "scripts": {
      "start": "electron .",
      "dev": "electron .", 
      "package": "electron-builder"
    },
    "keywords": [],
    "author": "Luis Agreda",
    "license": "Apache-2.0",
    "devDependencies": {
      "electron": "^28.2.0",
      "electron-builder": "^24.9.1"
    },
    "build": {
      "appId": "com.1111studiostech.datasetcotvisualizer",
      "productName": "Dataset CoT Visualizer",
      "copyright": "Copyright Â© 2032 Luis Agreda",
      "asar": true,
      "directories": {
        "output": "dist"
      },
      "files": [
        "**/*"
      ],
      "win": {
        "target": [
          "nsis"
        ],
        "icon": "build/icon.ico"
      },
      "mac": {
        "target": [
          "dmg"
        ],
        "icon": "build/icon.icns"
      },
      "linux": {
        "target": [
          "deb",
          "rpm",
          "AppImage"
        ],
        "icon": "build/icon.png"
      }
    }
  }